<Window x:Class="Bewegung_mit_den_Pfeiltasten.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bewegung_mit_den_Pfeiltasten"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="806" Name="window1" PreviewKeyDown="Window_PreviewKeyDown" FocusManager.FocusedElement="{Binding ElementName=canvas_Bewegung}">
    <Canvas x:Name="canvas_Bewegung" KeyDown="Canvas_Bewegung_KeyDown" Focusable="true" >
        <Ellipse x:Name="uwu" Height="50" Fill="DarkSeaGreen" Canvas.Left="10" Stroke="Black" 
                 Canvas.Bottom="10" Width="50"/>
        <Rectangle x:Name="Wand" Stroke="Black" Fill="Gray" Width="10" Height="200" Canvas.Left="260" Canvas.Bottom="50" Canvas.Top="105">

        </Rectangle>

    </Canvas>


</Window>
